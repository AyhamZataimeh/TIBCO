<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns:WriteToFileRequest="http://www.tibco.com/schemas/ProjectZero/Recources/Schemas/SharedProcesses/WriteToFileRequest"
	 xmlns:Header="http://www.tibco.com/schemas/ProjectZero/Recources/Schemas/Common/Header"
	 targetNamespace="http://www.tibco.com/schemas/ProjectZero/Recources/Schemas/SharedProcesses/WriteToFileRequest"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:import namespace="http://www.tibco.com/schemas/ProjectZero/Recources/Schemas/Common/Header" schemaLocation="../Common/Header.xsd"/>
	<xs:element name="WriteToFileRequest">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="WriteToFileRequest:Body"/>
				<xs:element ref="Header:Header"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="Body">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="message" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>