<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.tibco.com/schemas/ProjectZero/Recources/Schemas/Common/Header"
	 xmlns:Context="http://www.tibco.com/schemas/ProjectZero/Recources/Schemas/Common/Context"
	 targetNamespace="http://www.tibco.com/schemas/ProjectZero/Recources/Schemas/Common/Header"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:import namespace="http://www.tibco.com/schemas/ProjectZero/Recources/Schemas/Common/Context" schemaLocation="Context.xsd"/>
	<xs:element name="Header">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="timestamp" type="xs:dateTime"/>
				<xs:element ref="Context:Context"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>